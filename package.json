{
  "name": "NodePractice",
  "version": "1.0.0",
  "description": "Nodejs Practice",
  "main": "index.js",
  "author": "Likhith K.P.",
  "license": "MIT",
  "scripts": {
    "build": "node ./bootstrap/pull-secrets.js",
    "start": "node index.js",
    "local": "nodemon index.js",
    "pretest": "eslint --ignore-path .gitignore . --ignore-pattern **/__tests__/* ",
    "migrate": "sequelize db:migrate",
    "migrate:single": "sequelize db:migrate --name ${FILE_NAME}",
    "win-migrate:single": "sequelize db:migrate --name",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:single": "sequelize db:migrate:undo --name ${FILE_NAME}",
    "win-migrate:undo:single": "sequelize db:migrate:undo --name",
    "seed": "sequelize db:seed:all",
    "seed:undo:single": "sequelize db:seed:undo --seed [${FILE_NAME}]",
    "win-seed:undo:single": "sequelize db:seed:undo --seed",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "create-migration": "sequelize migration:create --name ${FILE_NAME}",
    "win-create-migration": "sequelize migration:create --name",
    "create-seeder": "sequelize seed:create --name ${FILE_NAME}",
    "lint": "eslint ./ --fix"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "graphql": "^16.8.1",
    "logger": "^0.0.1",
    "mongoose": "^8.2.1",
    "nodemon": "^3.1.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^8.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.13",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
